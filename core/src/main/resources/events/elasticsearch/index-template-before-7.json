{
  "settings": {
    "index": {
      "refresh_interval": "5s",
      "codec": "best_compression",
      "auto_expand_replicas": "0-2",
      "max_result_window": 10000,
      "max_inner_result_window": 100
    },
    "analysis": {
      "normalizer": {
        "lowercase_normalizer": {
          "type": "custom",
          "char_filter": [],
          "filter": [
            "lowercase"
          ]
        }
      }
    }
  },
  "aliases": {},
  "mappings": {
    "_doc": {
      "_source": {
        "enabled": false
      },
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "yyyy-MM-dd'T'HH:mm:ss.SSSZZ",
          "store": true
        },
        "name": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer",
          "store": true
        },
        "level": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer",
          "store": true
        },
        "tags": {
          "type": "object",
          "include_in_root": true
        },
        "message": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer",
          "store": true
        },
        "boolean": {
          "type": "boolean",
          "store": true
        },
        "non-finite-decimal": {
          "type": "keyword",
          "store": true
        },
        "number": {
          "type": "double",
          "store": true
        },
        "instant": {
          "type": "date",
          "format": "yyyy-MM-dd'T'HH:mm:ss.SSSSSSZZ || yyyy-MM-dd'T'HH:mm:ss.SSSSSS",
          "store": true
        },
        "error": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer",
          "store": true
        },
        "stack-trace": {
          "type": "text",
          "store": true
        },
        "duration": {
          "type": "long",
          "store": true
        },
        "unit": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer",
          "store": true
        },
        "latitude": {
          "type": "double",
          "store": true
        },
        "longitude": {
          "type": "double",
          "store": true
        },
        "lower-bound": {
          "type": "double",
          "store": true
        },
        "upper-bound": {
          "type": "double",
          "store": true
        },
        "values": {
          "type": "keyword",
          "store": true
        },
        "value": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "store": true
        }
      },
      "dynamic_templates": [
        {
          "tags": {
            "match_mapping_type": "string",
            "path_match": "tags.*",
            "mapping": {
              "type": "keyword",
              "normalizer": "lowercase_normalizer",
              "store": true
            }
          }
        }
      ]
    }
  }
}